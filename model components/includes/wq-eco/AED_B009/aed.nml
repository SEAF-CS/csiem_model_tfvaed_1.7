! aed_models  :  List of AED modules to be simulated
!-------------------------------------------------------------------------------
&aed_models
    models =  'aed_sedflux'        ,
              'aed_noncohesive'    ,
              'aed_oxygen'         ,
             !'aed_silica'         ,
              'aed_nitrogen'       ,
              'aed_phosphorus'     ,
              'aed_organic_matter' ,
              'aed_phytoplankton'  ,
	            'aed_tracer'         ,
	           !'aed_oasim' 
	            'aed_zooplankton'    ,
              'aed_macrophyte'     ,
	            'aed_macroalgae'     ,
	            'aed_bivalve'        ,
	            'aed_totals'         ,     
/

